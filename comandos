USUARIOS HUB/ITAU
usuario no hub/Itau com divida - '08889316705', 'GS7YBJB', '2001-01-01'

//Para pegar usuarios validos no chat/Hub itau
 SELECT DS_CPF_CNPJ_CCQP, DS_CODIGO_CCQP, DT_NASCIMENTO_UCQP    
 FROM CPF_CODIGO_QUEROPAGAR 
INNER JOIN USUARIO_CLIENTE_QUEROPAGAR ON (ID_USUARIO_CLIENTE_UCQP = ID_USUARIO_CLIENTE_CCQP)
 WHERE  ID_EMPRESA_CCQP = 306 and DT_NASCIMENTO_UCQP is not null;

//Para pegar usuario com dividas validas
SELECT 
  DS_CPF_CNPJ_CCQP,
    DS_CODIGO_CCQP,
    DT_NASCIMENTO_UCQP    
 FROM CPF_CODIGO_QUEROPAGAR 
INNER JOIN USUARIO_CLIENTE_QUEROPAGAR ON ID_USUARIO_CLIENTE_UCQP = ID_USUARIO_CLIENTE_CCQP
 WHERE  DS_CODIGO_CCQP = 'GS7YBJB'and 
 ID_EMPRESA_CCQP = 306 


